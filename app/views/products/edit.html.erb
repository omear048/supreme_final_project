<h1> Edit Item </h1>

<%= form_for(@product) do |f| %>
  <%= render 'shared/product_error_messages' %> 
 
  <div class ="field">
    <%= f.label :title %><br/>
    <%= f.text_area :title %><br/>
  <div>

  <div class ="field">
    <%= f.label :price %><br/>
    <%= f.text_area :price %><br/>
  <div>

  <div class ="field">
    <%= f.label :picture_url %><br/>
    <%= f.text_area :picture_url %><br/>
  <div>

  <div class="actions">
    <%= f.submit "Update Product" %>
  </div>
<% end %>
